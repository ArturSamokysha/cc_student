package aufgaben;

public class AufgabeVariablen {
	public static void main(String[] args) {
		//Deklaration
		byte byteZahl;
		//Zuweisung
		byteZahl = Byte.MAX_VALUE;
		System.out.print(byteZahl);
		
		//Deklaration und Zuweisung
		byte byteZahlMin = Byte.MIN_VALUE;
		System.out.print(byteZahlMin);
	}
}
