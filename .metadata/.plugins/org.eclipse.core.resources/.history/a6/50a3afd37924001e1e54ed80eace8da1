package def_package;
import java.util.Scanner;

public class ha_tag4 {
	public static void main(String[] args) {
		Scanner num = new Scanner(System.in);
		System.out.print("Cent: ");
		int x = num.nextInt();
		int euro = x / 100;
		int cent = x % 100;
		System.out.print(euro+" Euro "+cent+" Cent");
	}
}
